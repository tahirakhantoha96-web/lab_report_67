#include<stdio.h>
#include<string.h>
int main() {
    char str1[200],str2[100],result[300];
    int pos,i,j=0;
    printf("Enter first string:");
    scanf(" %[^\n]",str1);
    printf("Enter second string to insert:");
    scanf(" %[^\n]",str2);
    printf("Enter position to insert:");
    scanf("%d",&pos);
    for(i=0;i<pos&&str1[i]!='\0';i++)
        result[i]=str1[i];
    for(j=0;str2[j]!='\0';j++)
        result[i++]=str2[j];
    for(;str1[pos]!='\0';pos++)
        result[i++]=str1[pos];
    result[i] ='\0';
    printf("Resulting string: %s\n",result);
    return 0;
}
